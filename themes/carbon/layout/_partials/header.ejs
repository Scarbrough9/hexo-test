<header class="site-header">
    <h3>Header</h3>
    <% var menu = config.menus.primary; %>
    <% if(menu){ %>
    <nav>
        <ul class="menu">
            <% for (var i in menu){ %>
                <li class="menu-item">
                    <a class="menu-link" title="<%= i %> | <%= config.title %>" data-type="page-transition" href="<%- url_for(menu[i]) %>"><%= i %></a>
                </li>
            <% } %>
        </ul>
    </nav>
    <% } %>
</header>
